{
  "name": "wdio",
  "version": "1.0.0",
  "description": "testing via WDIO",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio run ./wdio.conf.js",
    "wdio": "wdio run ./wdio.conf.js",
    "task_01": "npx wdio run ./config/wdio.conf.js --suite task_01",
    "task_02": "npx wdio run ./config/wdio.conf.js --suite task_02",
    "task_03": "npx wdio run ./config/wdio.conf.js --suite task_03",
    "task_04": "npx wdio run ./config/wdio.conf.js --suite task_04",
    "smokeTest": "npx wdio run ./config/wdio.conf.js --suite smokeTest",
    "smokeTest_chrome": "npx wdio run ./config/wdio.conf.js --suite smokeTest .config/yargs.conf.js --browser chrome",
    "smokeTest_edge": "npx wdio run ./config/wdio.conf.js --suite smokeTest .config/yargs.conf.js --browser edge",
    "otherTests": "npx wdio run ./config/wdio.conf.js --suite otherTests"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.27.2",
    "@wdio/cli": "^8.27.2",
    "@wdio/local-runner": "^8.27.2",
    "@wdio/mocha-framework": "^8.27.2",
    "@wdio/spec-reporter": "^8.27.2",
    "chromedriver": "^120.0.1",
    "edgedriver": "^5.3.9",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-edgedriver-service": "^3.0.3",
    "yargs": "^17.7.2"
  }
}
